<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.demo.struts1.model.Spotify" table="spotify_song">
        <id name="id" column="id">
            <!--            <generator class="increment" />-->
        </id>
        <property name="trackName" column="track_name" not-null="true" />
        <property name="artistName" column="artist_name" not-null="true" />
        <property name="genre" column="genre" not-null="true" />
        <property name="popularity" column="popularity" not-null="true" />
    </class>

</hibernate-mapping>
